{
  "name": "@vuikit/vuikit",
  "version": "0.7.15",
  "description": "A Vuejs component library based on UIkit",
  "license": "MIT",
  "author": {
    "name": "Miljan Aleksic",
    "url": "https://twitter.com/AleksicMiljan"
  },
  "homepage": "https://vuikit.js.org/",
  "bugs": "https://github.com/vuikit/vuikit/issues",
  "keywords": [
    "library",
    "vue",
    "vuejs",
    "uikit",
    "vuikit"
  ],
  "main": "dist/vuikit.cjs.js",
  "module": "dist/vuikit.esm.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "build": "yarn run build:lib && yarn run build:dist",
    "build:lib": "node -r @std/esm scripts/build-lib.js",
    "build:dist": "node -r @std/esm scripts/build-dist.js"
  },
  "devDependencies": {
    "@lump/argv": "^0.1.0",
    "@lump/banner": "^0.2.0",
    "@lump/remove": "^0.1.0",
    "@lump/rollup": "^0.3.5",
    "@lump/write": "^0.2.4",
    "@std/esm": "^0.18.0",
    "globby": "^7.1.1",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-cleanup": "^2.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-vue": "^3.0.0"
  },
  "peerDependencies": {
    "@vuikit/core": "^0.3.2",
    "vue": "^2.5.11"
  },
  "@std/esm": {
    "esm": "js",
    "cjs": true
  }
}
