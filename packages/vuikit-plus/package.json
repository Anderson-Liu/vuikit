{
  "name": "@vuikit/plus",
  "version": "0.5.0",
  "description": "Additional Vuikit components not part of UIkit collection",
  "author": {
    "name": "Miljan Aleksic",
    "url": "https://twitter.com/AleksicMiljan"
  },
  "license": "MIT",
  "homepage": "https://vuikit.js.org/",
  "bugs": "https://github.com/vuikit/vuikit/issues",
  "keywords": [
    "vuikit",
    "vuejs",
    "uikit",
    "ui"
  ],
  "main": "dist/vuikit-plus.cjs.js",
  "module": "dist/vuikit-plus.esm.js",
