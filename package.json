{
  "name": "@vuikit/vuikit",
  "version": "0.7.0",
  "description": "UIkit with all the power of Vue",
  "author": {
    "name": "Miljan Aleksic",
    "url": "https://twitter.com/AleksicMiljan"
  },
  "keywords": [
    "library",
    "vue",
    "vuejs",
    "uikit",
    "vuikit"
  ],
  "main": "dist/vuikit.common.js",
  "module": "dist/vuikit.esm.js",
  "files": [
    "dist",
    "src",
    "helpers"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuikit/vuikit.git"
  },
  "bugs": {
    "url": "https://github.com/vuikit/vuikit/issues"
  },
  "homepage": "https://github.com/vuikit/vuikit",
  "scripts": {
    "build": "node build/build.js",
    "dev": "start-storybook -p 6006",
    "deploy": "cd build && ./deploy.sh",
    "test": "yarn precompile && yarn build && ./node_modules/.bin/jest",
    "precompile": "node build/precompile-helpers.js"
  },
  "dependencies": {
    "@vuikit/icons": "^0.1.0",
    "@vuikit/util": "^0.1.0",
    "vue": "^2.4.4"
  },
  "devDependencies": {
    "@lump/banner": "^0.1.1",
    "@lump/it": "^0.1.0",
    "@lump/less": "^0.1.0",
    "@lump/minify": "^0.1.0",
    "@lump/remove": "^0.1.0",
    "@lump/rollup": "^0.1.0",
    "@lump/task": "^0.1.0",
    "@storybook/vue": "^3.2.4",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.0",
    "babel-preset-vue-app": "^1.2.0",
    "date-fns": "^1.28.5",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^20.0.0",
    "mockdate": "^2.0.1",
    "poi": "^9.1.6",
    "popper.js": "^1.0.8",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-vue": "^2.4.1",
    "uikit": "^3.0.0-beta.27",
    "vue-loader": "^13.0.2",
    "vue-template-compiler": "^2.4.4"
  },
  "jest": {
    "testRegex": "/tests/.*(test|spec)\\.js$",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      "^vuikit$": "<rootDir>/dist/vuikit.esm.js",
      "tests/(.*)$": "<rootDir>/tests/$1.js",
      "helpers/(.*)$": "<rootDir>/helpers/$1.js",
      "src/(.*)$": "<rootDir>/src/$1.js"
    },
    "snapshotSerializers": [
      "<rootDir>/tests/snapshotBeautifier.js"
    ]
  },
  "engines": {
    "node": ">= 7.8.0",
    "npm": ">= 3.0.0"
  }
}
