{
  "name": "vuikit",
  "version": "0.7.0",
  "description": "UIkit with all the power of Vue",
  "author": {
    "name": "Miljan Aleksic",
    "url": "https://twitter.com/AleksicMiljan"
  },
  "keywords": [
    "library",
    "vue",
    "vuejs",
    "uikit",
    "vuikit"
  ],
  "main": "dist/js/vuikit.common.js",
  "module": "dist/js/vuikit.esm.js",
  "files": [
    "dist",
    "src",
    "helpers"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuikit/vuikit.git"
  },
  "bugs": {
    "url": "https://github.com/vuikit/vuikit/issues"
  },
  "homepage": "https://github.com/vuikit/vuikit",
  "scripts": {
    "build": "node build/prod/build.js",
    "dev": "node build/dev/server.js",
    "deploy": "cd build && ./deploy.sh",
    "test": "yarn precompile && yarn build && ./node_modules/.bin/jest",
    "precompile": "node build/precompile-helpers.js",
    "explorer": "poi --config explorer/poi.config.js"
  },
  "dependencies": {
    "vue": "^2.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-stage-2": "^6.24.1",
    "clean-css": "^4.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.28.1",
    "date-fns": "^1.28.4",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.28.0",
    "inject-loader": "^3.0.0",
    "jest": "^20.0.0",
    "jquery": "^3.2.1",
    "js-beautify": "^1.6.12",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "less-plugin-clean-css": "^1.5.1",
    "lodash-es": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.3",
    "mkdirp": "^0.5.1",
    "mockdate": "^2.0.1",
    "ora": "^1.2.0",
    "poi": "^8.0.0-rc.7",
    "popper.js": "^1.0.8",
    "portfinder": "^1.0.13",
    "rmrf": "^1.0.2",
    "rollup": "^0.41.6",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.2",
    "rollup-plugin-vue": "^2.3.1",
    "semver": "^5.3.0",
    "shelljs": "^0.7.7",
    "tween.js": "^16.6.0",
    "uikit": "^3.0.0-beta.22",
    "url-loader": "^0.5.8",
    "vue-loader": "^12.0.3",
    "vue-router": "^2.5.3",
    "vue-server-renderer": "^2.3.2",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.2",
    "vuikit-docs": "^0.1.1",
    "vuikit-explorer": "^0.0.3",
    "webpack": "^2.5.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "testRegex": "/tests/.*(test|spec)\\.js$",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      "^vuikit$": "<rootDir>/dist/js/vuikit.esm.js",
      "tests/(.*)$": "<rootDir>/tests/$1.js",
      "helpers/(.*)$": "<rootDir>/helpers/$1.js",
      "src/(.*)$": "<rootDir>/src/$1.js"
    },
    "snapshotSerializers": [
      "<rootDir>/tests/snapshotBeautifier.js"
    ]
  },
  "engines": {
    "node": ">= 7.8.0",
    "npm": ">= 3.0.0"
  }
}
