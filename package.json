    "build:lib": "node -r @std/esm _internal/build-lib.js"
  },
  "dependencies": {
    "vue": "^2.5.3"
  },
  "devDependencies": {
    "@lump/argv": "^0.1.0",
    "@lump/banner": "^0.2.0",
    "@lump/it": "^0.1.1",
    "@lump/minify": "^0.2.0",
    "@lump/remove": "^0.1.0",
    "@lump/rollup": "^0.3.5",
    "@lump/size": "^0.1.0",
    "@lump/task": "^0.2.0",
    "@lump/write": "^0.2.4",
    "@std/esm": "^0.15.0",
    "@vuikit/core": "^0.3.0",
    "@vuikit/icons": "^0.5.0",
    "@vuikit/theme": "^0.1.3",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "lodash": "^4.17.4",
    "poi": "^9.5.1",
    "rollup": "^0.51.5",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-buble": "^0.17.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-vue": "^2.5.2",
    "vue-loader": "^13.5.0",
    "vue-play": "^3.2.1",
    "vue-template-compiler": "^2.5.3"
  },
  "@std/esm": {
    "esm": "js",
    "cjs": true
  },
  "engines": {
    "node": ">= 7.8.0",
    "npm": ">= 3.0.0"
  }
