{
  "name": "vuikit",
  "version": "0.7.0",
  "description": "UIkit with all the power of Vue",
  "author": {
    "name": "Miljan Aleksic",
    "url": "https://twitter.com/AleksicMiljan"
  },
  "keywords": [
    "library",
    "vue",
    "vuejs",
    "uikit",
    "vuikit"
  ],
  "main": "dist/js/vuikit.common.js",
  "module": "dist/js/vuikit.esm.js",
  "files": [
    "dist",
    "src",
    "helpers",
    "utils"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuikit/vuikit.git"
  },
  "bugs": {
    "url": "https://github.com/vuikit/vuikit/issues"
  },
  "homepage": "https://github.com/vuikit/vuikit",
  "scripts": {
    "build": "npm run clean-dist && npm run compile",
    "compile": "npm run compile-lib && npm run compile-theme && npm run compile-theme-min",
    "compile-lib": "node build/build.js",
    "compile-theme": "./node_modules/.bin/lessc --relative-urls src/less/theme.less dist/css/vuikit.css",
    "compile-theme-min": "./node_modules/.bin/lessc --clean-css --source-map --relative-urls src/less/theme.less dist/css/vuikit.min.css",
    "clean-dist": "rm -rf dist && mkdir -p dist/js && mkdir -p dist/css && mkdir -p dist/images",
    "dev": "node dev/server.js",
    "deploy": "cd build && ./deploy.sh",
    "test": "./node_modules/.bin/jest",
    "lint": "eslint --ext .js,.vue src test/unit/specs"
  },
  "dependencies": {
    "vue": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.26.2",
    "date-fns": "^1.28.0",
    "eslint": "^3.17.0",
    "eslint-config-standard": "^7.0.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.28.0",
    "inject-loader": "^3.0.0-beta4",
    "jest": "^19.0.2",
    "jquery": "^3.1.1",
    "js-beautify": "^1.6.11",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "less-plugin-clean-css": "^1.5.1",
    "lodash-es": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.2",
    "mockdate": "^2.0.1",
    "ora": "^1.1.0",
    "popper.js": "^1.0.7",
    "portfinder": "^1.0.13",
    "rollup": "^0.41.4",
    "rollup-plugin-alias": "^1.2.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-plugin-vue": "^2.2.21",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "tween.js": "^16.6.0",
    "uikit": "^3.0.0-beta.18",
    "url-loader": "^0.5.8",
    "vue-loader": "^11.1.4",
    "vue-router": "^2.3.0",
    "vue-server-renderer": "^2.2.1",
    "vue-style-loader": "^2.0.3",
    "vue-template-compiler": "^2.2.1",
    "vuikit-docs": "^0.1.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "^3.0.0"
  },
  "jest": {
    "testRegex": "/__tests__/.*(test|spec)\\.jsx?$",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      "^vuikit$": "<rootDir>/dist/js/vuikit.esm.js",
      "jest/(.*)$": "<rootDir>/jest/$1.js",
      "helpers/(.*)$": "<rootDir>/helpers/$1.js",
      "utils/(.*)$": "<rootDir>/utils/$1.js"
    },
    "snapshotSerializers": [
      "<rootDir>/jest/snapshotBeautifier.js"
    ]
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  }
}
